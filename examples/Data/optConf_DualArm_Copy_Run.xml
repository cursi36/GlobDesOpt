<optDesConf
    solver="BayesOpt"
Accept_rate="0.6"
MaxIter="100"
Npnts_WS="100*1e03"
popSize="50"
NumSeed_BO="1000"
> 
    
    <Robot1 nj="5" origin="[0 0 0]" rpy="[0 0 0]">
        <DH_tab_path name="DH_tab_DualArm_Copy_Run.txt" /> 
        <joints 
            types="prrrr"
            l_bounds="[0 -45 -45 -45 -45]"
            u_bounds="[10 45 45 45 45]"/>
        <d_idx idx="[4 5]"
               l_bounds="[3 3]"
               u_bounds="[25 25]"/>
        <theta_idx idx="[4 5]"        
                   l_bounds="[0 0]"
                   u_bounds="[180 180]"/>
        <a_idx idx="[1 3 4 5]"         
               l_bounds="[3 3 3 3]"
               u_bounds="[25 25 25 25]"/>
        <alpha_idx idx="[4 5]"         
                   l_bounds="[0 0]"
                   u_bounds="[180 180]"/>
        <jnt_idx idx="[4 5]"         
                 l_p_bounds="[0 0]"
                 u_p_bounds="[1 1]"
                 l_r_bounds="[-45 -45]"
                 u_r_bounds="[45 45]"/>
    </Robot1>
    
    <dual_arm
        copy="true" 
        dist_idx="[1]"
        l_bounds ="[0]"
        u_bounds = "[25]"
        />
        
    <!-- Robot 2 only set its initial pose -->
    <Robot2 nj="5" origin="[5 0 0]" rpy="[0 0 0]"/>
    
</optDesConf>